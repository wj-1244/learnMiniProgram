<!--pages/input/input.wxml-->
<!-- input基本使用 -->
<input></input>
<!-- 默认值 -->
<input value="111"></input>
<!-- 决定键盘类型（文本，数字，身份证，带小数点的数字键盘） -->
<input type="number"></input>
<!-- password暗文 -->
<input password></input>
<!-- 占位文字 -->
<input placeholder="111"></input>
<!-- 绑定事件 -->
<input bindblur="handleBlur" bindfocus="handleFocus" bindinput="handleInput"></input>